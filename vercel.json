{
  "version": 2,
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/client",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
