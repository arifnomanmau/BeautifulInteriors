{
  "version": 2,
  "functions": {
    "api/login.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/user.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/login", "dest": "/api/login.js" },
    { "src": "/api/user", "dest": "/api/user.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
